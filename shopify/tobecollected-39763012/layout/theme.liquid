<body id="{{ template }}">

  <header class="site-header">
    <div class="inner">
      <h1 class="site-header__logo"><a href="{{ shop.url }}">{{ shop.name }}</a></h1>
      <div class="site-header__navigation">
        <div class="site-header__cart">
          <a href="{{ shop.url }}/cart">Cart <span class="spacer">|</span> <span id="cart-count" class="cart-count">{{ cart.item_count }}</span></a>
          {{ 'layout.header.cart' | t }}
        </div>
        <nav class="site-header__collections">
          <span class="nav-toggle">Collections</span>
          <ol>
            <li><a href="index.php">Full Collection</a></li>
            <li><a href="index.php">Small Goods</a></li>
          </ol>
        </nav>
      </div>
      {% if template == 'index' %}
      <aside class="site-header__content">
        <div class="group">
          <img src="{{ settings.home_image_one | asset_url }}" alt="photo">
          <div class="text-wrapper">
            <h1>{{ settings.home_headline }}</h1>
            <p>{{ settings.home_text }}</p>
          </div>
        </div>
        <div class="group">
          <img src="{{ settings.home_image_two | asset_url }}" alt="photo">
        </div>
      </aside>
      {% endif %}
    </div>
