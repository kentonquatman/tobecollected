        {%if settings.pinterest != '' %}<li class="pinterest"><a href="{{ settings.pinterest }}" target="_blank">Pinterest</a></li>{% endif %}
      </ul>
      <p class="site-footer__copyright">&copy; {{ shop.name }}</p>
